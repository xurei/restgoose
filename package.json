{
  "name": "@xureilab/restgoose",
  "version": "0.0.10",
  "description": "Model-driven REST API framework",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "node_modules/.bin/rimraf lib && npm run tslint && npm run build-ts",
    "build-ts": "node_modules/.bin/tsc -p .",
    "build-examples": "node_modules/.bin/tsc -p examples",
    "tslint": "node_modules/.bin/tslint -p .",
    "test": "node_modules/.bin/nyc mocha --opts ./mocha.opts test/*.ts",
    "coverage": "node_modules/.bin/nyc report --reporter=json && node_modules/.bin/codecov -f coverage/*.json"
  },
  "keywords": [
    "REST",
    "mongodb",
    "decorators",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/xurei/restgoose.git"
  },
  "author": "Olivier Bourdoux",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.0.0"
  },
  "devDependencies": {
    "@types/es6-promise": "3.3.0",
    "@types/express": "4.16.0",
    "@types/mocha": "2.2.48",
    "@types/mongoose": "5.3.1",
    "@types/node": "8.10.0",
    "chai": "4.2.0",
    "chai-match-pattern": "1.1.0",
    "codecov": "3.1.0",
    "cors": "2.8.5",
    "dirty-chai": "2.0.1",
    "express": "4.16.4",
    "jsonwebtoken": "8.4.0",
    "mocha": "5.2.0",
    "mock-req": "0.2.0",
    "moment": "2.23.0",
    "mongoose": "5.3.13",
    "nyc": "13.1.0",
    "promise": "8.0.2",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "run-middleware": "0.9.10",
    "sinon-chai": "3.2.0",
    "ts-node": "3.3.0",
    "ts-sinon": "1.0.12",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "debug": "^3.2.6",
    "typegoose": "^5.4.0"
  }
}
